package com.graburjob.designpatterns.stategy.problem;

public class Client {

	public static void main(String[] args) {
		// Driving
		Vehicle vehicle = new Car();
		vehicle.go();
		// Driving
		vehicle = new Bus();
		vehicle.go();
		// Driving Fast
		vehicle = new FormulaOne();
		vehicle.go();
		
		// Flying ... not Driving
		vehicle = new FormulaOne();
		vehicle.go();
	}

}
